		<typeAlias type="com.jscompany.springproject.model.domain.lecture.Memberhavelecture" alias="Memberhavelecture" />
		<typeAlias type="com.jscompany.springproject.model.domain.board.Notice" alias="Notice" />
		<typeAlias type="com.jscompany.springproject.model.domain.board.Qna" alias="Qna" />
		<typeAlias type="com.jscompany.springproject.model.domain.board.Qna_Comment" alias="Qna_Comment" />
	</typeAliases>
	<mappers>
		<mapper	resource="com/jscompany/springproject/mybatis/mariadbMappers/MemberMapper.xml" />
		<mapper	resource="com/jscompany/springproject/mybatis/mariadbMappers/Lecture_TopCategoryMapper.xml" />
		<mapper	resource="com/jscompany/springproject/mybatis/mariadbMappers/Lecture_SubCategoryMapper.xml" />
