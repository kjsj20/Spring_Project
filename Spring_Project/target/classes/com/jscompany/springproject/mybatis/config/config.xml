		<mapper	resource="com/jscompany/springproject/mybatis/mariadbMappers/Section_DetailMapper.xml" />
		<mapper	resource="com/jscompany/springproject/mybatis/mariadbMappers/MemberhavelectureMapper.xml" />
		<mapper	resource="com/jscompany/springproject/mybatis/mariadbMappers/NoticeMapper.xml" />
		<mapper	resource="com/jscompany/springproject/mybatis/mariadbMappers/QnaMapper.xml" />
