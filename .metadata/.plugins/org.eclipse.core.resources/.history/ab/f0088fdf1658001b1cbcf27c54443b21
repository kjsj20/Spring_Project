package com.jscompany.springproject.model.lecture.repository;

import org.mybatis.spring.SqlSessionTemplate;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Repository;

@Repository
public class MybatisMemberhavelectureDAO implements MemberhavelectureDAO{
	
	@Autowired
	private SqlSessionTemplate sqlSessionTemplate;
	
	public int courseInsert(int member_id, int lecture_id) throws Exception {
		return sqlSessionTemplate.insert("Memberhavelecture.insert", );
	}

}
