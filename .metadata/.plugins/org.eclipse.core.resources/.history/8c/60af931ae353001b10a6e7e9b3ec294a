$(function(){
	$('.delMember').click(function(){
		if(confirm("정말 삭제 하시겠습니까?")){
			delMember(this.id);
		};
	});
});

function delMember(id){
	$.ajax({
		url:"/admin/delMember",
		type:"POST",
		data: id,
		success:function(data){
			alert('삭제성공');
		},
		error:function(){
			alert('삭제중 오류 발생');
		}
	});
}